@model IEnumerable<Parser.Data.ViewModels.Administration.LogEntryViewModel>
@{
    ViewBag.Title = "Administration";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 style="color: yellowgreen; text-align:center;">Administration</h1>

<div class="well" id="log-entries">

    @if (Model.Count() == 0)
    {
        <h2 class="info">No error occurred.</h2>
    }
    else
    {
        @Html.Partial("_LogEntryViewModelPartial", Model)
    }
</div>
