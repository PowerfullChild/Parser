@model IEnumerable<Parser.Data.ViewModels.Administration.LogEntryViewModel>

@if (Model.Count() == 0)
{
    <h2 class="info">No errors occurred.</h2>
}
else
{
    <table>
        <tr>
            <th>Time Stamp</th>
            <th>Message</th>
            <th>Controller</th>
            <th>Action</th>
        </tr>

        @foreach (var viewModel in Model)
        {
            <tr>
                <td>@viewModel.Timestamp.ToShortTimeString()</td>
                <td>@viewModel.Message</td>
                <td>@viewModel.Controller</td>
                <td>@viewModel.Action</td>
            </tr>
        }
    </table>
}